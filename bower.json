{
  "name": "fullcrypto",
  "description": "A client-side encryption suite for plain-text, HTML pages, or files.",
  "main": "index.html",
  "authors": [
    ""
  ],
  "license": "MIT",
  "keywords": [
    "Encryption",
    "Crypto"
  ],
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "^3.3.7",    
    "crypto-js": "^3.1.9",    
    "jquery": "^3.2.1",
    "spinkit": "^1.2.5"
  },
  "install": {
    "base": "www",
    "path": {
      "js": "{name}/js",
      "css": "{name}/css",
      "eot": "{name}/fonts",
      "svg": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts",
      "woff2": "{name}/fonts"
    },
    "sources": {
      "bootstrap": [
        "bower_components/bootstrap/dist/css/*.min.css",
        "bower_components/bootstrap/dist/fonts/**",
        "bower_components/bootstrap/dist/js/*.min.js"
      ],
      "crypto-js": [
        "bower_components/crypto-js/aes.js",
        "bower_components/crypto-js/crypto-js.js",
        "bower_components/crypto-js/sha256.js"
      ],
      "jquery": [
        "bower_components/jquery/dist/*.min.js"
      ],
      "spinkit": [
        "bower_components/spinkit/css/**"
      ]
    }
  }
}
